{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "globals": {
        "ComfyJS": true,
        "Queue": true
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "linebreak-style": "off"
    }
}